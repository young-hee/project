{{#each this.brandCards}}
	<div class="section brand_new01">
		<dl class="brand_info">
			<dt>
				<a href="/display/brand/{{ this.displayMenuId }}?displayMenuId={{ this.displayMenuId }}?brandSn={{ this.brandSn }}">
					{{#each this.logos}}
						{{#xif this.logoType '==' 'BrandLogo01'}}
							<img class="brand" alt="{{ ../brandName }}" src="{{ absolutePath this.fileUrl }}"><span>{{ ../brandName }}</span><i class="ico_arr"></i>
						{{/xif}}
					{{/each}}
				</a>
				<button type="button" class="btn_toggle like_btn"><i class="ico_heart_s {{#xif this.memberShoppingMarkYn '==' 'Y'}}on{{/xif}}"></i><span class="sr_only">좋아요</span></button>
			</dt>
			<dd>
				{{#xif this.planDisplayExistYn '==' 'Y'}}
					<div class="display_brand_list01">
						<ul>
							{{#each this.planDisplayInfos}}
								<li><a href="javascript:;"><img alt="{{ this.planDisplayTitle }}" ap:src="@{/images/dummy/img_item_01.png}"></a></li>
							{{/each}}
						</ul>
					</div>
				{{else}}
					<div class="slide" data-ix-options="auto-play:true; paging:true; view-length:1;">
						<div class="ix-list-viewport">
							<ul class="ix-list-items">
								{{#each this.prods.list}}
									{{#xif @index '<' 6}}
										<li class="ix-list-item">
											<a href="{{ productDetailPath this.onlineProdSn }}">
												<img alt="{{ this.onlineProdName }}" src="{{ absolutePath onlineProdImages.[0].imgUrl }}">
											</a>
										</li>
									{{/xif}}
								{{/each}}
							</ul>
						</div>
						<div class="ix-controller white_overlay">
							<div class="paging"><strong class="current"></strong> / <span class="total"></span></div>
							<button type="button" class="btn_view_all">전체보기</button>
						</div>
					</div>
				{{/xif}}

				<div class="tags scrollable_x">
					{{#each this.brandKeywordLinks}}
						<a href="{{ this.keywordLinkUrl }}">#{{ this.keywordLinkTitle}}</a>
					{{/each}}
				</div>
			</dd>
		</dl>
</div>
{{/each}}
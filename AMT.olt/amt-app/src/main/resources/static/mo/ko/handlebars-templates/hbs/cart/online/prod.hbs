														{{#case "icon_award_gift"}} <span class="cart_item_exclusive">임직원할인</span> {{/case}}
													{{/switch}}
												{{/each}}
											</div>
										{{/xif}}
										<span class="product_name"><a href="#none" onclick="fnProdDetail({{ onlineProdSn }})">{{ onlineProdName }}</a></span>
									</span>
								</label>
							</div>

			<dl class="tester_process">
				<dt>{{this.regularEvent.regularEventName}}</dt>
				<dd>
					<p class="text"></p>
					<ul class="step">
						
						{{#xif this.status '==' 'RQ'}}<li class="on">{{/xif}}
						{{#xif this.status '==' 'TN'}}<li class="done">{{/xif}}
						{{#xif this.status '==' 'RR'}}<li class="done">{{/xif}}
						{{#xif this.status '==' 'BR'}}<li class="done">{{/xif}}
							<span>참가신청</span>
							<i class="ico"></i>
							<span class="date">{{dateFormat this.regularEvent.requestStartDt "MM.DD"}}~{{dateFormat this.regularEvent.requestEndDt "MM.DD"}}</span>
							{{#xif this.status '==' 'RQ'}}
								<span class="sr_only">현재단계</span>
							{{/xif}}
						</li>
						
						{{#xif this.status '==' 'RQ'}}<li>{{/xif}}
						{{#xif this.status '==' 'TN'}}<li class="on">{{/xif}}
						{{#xif this.status '==' 'RR'}}<li class="done">{{/xif}}
						{{#xif this.status '==' 'BR'}}<li class="done">{{/xif}}
							<span>테스터발표</span>
							<i class="ico"></i>
							<span class="date">{{dateFormat this.regularEvent.winnerNoticeExpectedDt "MM.DD"}}</span>
							{{#xif this.status '==' 'TN'}}
								<span class="sr_only">현재단계</span>
							{{/xif}}
						</li>
						
						{{#xif this.status '==' 'RQ'}}<li>{{/xif}}
						{{#xif this.status '==' 'TN'}}<li>{{/xif}}
						{{#xif this.status '==' 'RR'}}<li class="on">{{/xif}}
						{{#xif this.status '==' 'BR'}}<li class="done">{{/xif}}
							<span>리뷰등록</span>
							<i class="ico"></i>
							{{#xif this.regularEvent.reviewRegistStartDt '!=' null}}
							<span class="date">{{dateFormat this.regularEvent.reviewRegistStartDt "MM.DD"}}~{{dateFormat this.regularEvent.reviewRegistEndDt "MM.DD"}}</span>
							{{/xif}}
							{{#xif this.status '==' 'RR'}}
								<span class="sr_only">현재단계</span>
							{{/xif}}
						</li>
						
						{{#xif this.status '==' 'RQ'}}<li>{{/xif}}
						{{#xif this.status '==' 'TN'}}<li>{{/xif}}
						{{#xif this.status '==' 'RR'}}<li>{{/xif}}
						{{#xif this.status '==' 'BR'}}<li class="on">{{/xif}}
							<span>베스트리뷰발표</span>
							<i class="ico"></i>
							<span class="date">{{dateFormat this.regularEvent.bestReviewNoticeDt "MM.DD"}}</span>
							{{#xif this.status '==' 'BR'}}
								<span class="sr_only">현재단계</span>
							{{/xif}}
						</li>
					</ul>
				</dd>
			</dl>
<dt class="on"><button type="button">{{condition (eq this.step 'two') this.title ''}} 사은품</button></dt>
<dd>
	<!-- 사은품 목록 -->
	{{#each fList}}
		<div class="panel_box">
			<div class="product">
				<!-- <div class="thumb"><img ap:src="@{/images/dummy/650000956_IM_01.png}" alt=""></div> -->
				<div class="info">
					<div class="align_left">
						<p class="name">{{this.promoNameBlang}}</p>
						<p class="flag">{{ordFreebiesCode 'promo' this.awardTgtCode}}</p>
					</div>
				</div>
			</div>
		</div>
	{{/each}}
	<!-- //사은품 목록 -->

	<!-- 쿠폰 목록 -->
	{{#each cList}}
		<div class="panel_box">
			<div class="product">
				<!-- <div class="thumb"><img ap:src="@{/images/dummy/650000956_IM_01.png}" alt=""></div> -->
				<div class="info">
					<div class="align_left">
						<p class="name">{{couponNameBlang}}</p>
						<p class="flag">{{ordFreebiesCode 'coupon' couponBenefitTypeCode}}</p>
					</div>
				</div>
			</div>
		</div>
	{{/each}}
	<!-- //쿠폰 목록 -->
</dd>
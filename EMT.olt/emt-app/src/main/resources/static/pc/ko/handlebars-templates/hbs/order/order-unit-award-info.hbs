							<li>
								<span class="check_wrap">
									<input type="radio" name="freebies0" id="{{this.ordUnitAwardSn}}" data-prodSn="{{this.ordProdEx.prodSn}}">
									<label for="{{this.ordUnitAwardSn}}"><span class="img"><img ap:src="{{this.ordProdEx.onlineProdImgUrl}}" alt=""></span>
										<span class="title">{{this.ordProdEx.prodNameBlang}} {{currencyFormatDefault this.ordProdEx.prodSalePrice '원'}}</span>
									</label>
								</span>
							</li>
						{{/each}}

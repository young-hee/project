				<div class="title_area">
					<h4 class="h_title sub">첫 구매 사은품</h4>
				</div>
				<ul class="freebies_list">
					{{#xif this.ordUnitAwardExPointList.length '>' 0}}
						{{#each this.ordUnitAwardExPointList}}
							<li>
								<span class="check_wrap">
									<input type="radio" name="freebies1" id="{{this.ordUnitAwardSn}}" data-methodCode="{{this.freeGiftAwardMethodCode}}">
									<label for="{{this.ordUnitAwardSn}}"><span class="img"><img ap:src="@{/images/cart/ico_beautypoint.png}" alt=""></span>
										<span class="title">{{pointTypeSwitch this.pointTypeCode}} {{this.savingPoint}}P</span>
									</label>
								</span>
							</li>
						{{/each}}
					{{/xif}}
					{{#xif this.ordUnitAwardExCouponList.length '>' 0}}
						{{#each this.ordUnitAwardExCouponList}}
							<li>
								<span class="check_wrap">
									<input type="radio" name="freebies0" id="{{this.ordUnitAwardSn}}" data-couponSn="{{this.couponSn}}">
									<label for="{{this.ordUnitAwardSn}}"><span class="img"><img ap:src="@{/images/cart/ico_beautypoint.png}" alt=""></span>
										<span class="title">{{this.couponName}} {{this.couponTypeCode}}</span>
									</label>
								</span>
							</li>
						{{/each}}
					{{/xif}}
					{{#xif this.ordUnitAwardExProdList.length '>' 0}}

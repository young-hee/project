{{#xif downloadCoupon.couponCnt '>' 0}}
	<div class="" data-open-type="single">
		<!--다운가능한 쿠폰리스트(MyPage참조)-->
		{{#each downloadCoupon.list}}
			<div class="coupon_order">
				<div class="coupon_wrap">
					<div class="check_wrap">
						<input type="checkbox" id="{{ this.couponSn }}" name="couponSn" data-code="{{ this.duplicationUseConditionCode }}">
						<label for="{{ this.couponSn }}"></label>
						<!--<span><button type="button" id="{{ this.couponSn }}" name="downloadCoupon" class="btn_sm_neutral">쿠폰받기</button></span>-->
					</div>
					<div class="info">
						<div class="flag_wrap">
							<span class="flag">{{ marketingOrg this.mktgOrgCode }}</span>
							<span class="flag">D-3</span>
						</div>
						<span class="coupon_name">{{ this.couponName }}</span>
						<div class="price_wrap">
							<span class="price">{{{ downloadCouponBenefitTypeCodeSwitch this }}}</span>	 <!-- //가격 숫자일때는 클래스 num -->
							<div class="ui_tooltip"><!-- //툴팁 -->
								<button type="button" class="btn_tooltip">툴팁보기</button>
								<span class="arr"></span>
								<div class="layer_tooltip">
									<dl>
										<dt>쿠폰 안내 <small>(만료예정일:{{ dateFormat this.issueEndDt }})</small></dt>
										<dd>
											<ul class="list_bullet_dot">
												<li><pre>{{ this.foGuide }}</pre></li>
											</ul>
											<button type="button" class="ui_close sm"><span class="sr_only">툴팁 닫기</span></button>
										</dd>
									</dl>
								</div>
							</div><!-- //툴팁 -->
						</div><!-- //price_wrap -->
					</div><!-- //info -->
				</div><!-- //coupon_wrap -->
			</div>
		{{/each}}
	</div>
	<div class="page_btns">
		<button class="btn_lg_secondary" type="button" id="b_close">취소</button>
		<button class="btn_lg_neutral" type="button" id="b_download">쿠폰받기</button>
	</div>
{{else}}
	<div class="panel notice downloadCouponEmpty">
		<i class="ico"></i>
		<p class="text"><b>다운로드 가능한 쿠폰이 없습니다.</b></p>
		<br>
		<p class="text">이벤트/행사 정보를 통해 할인 및 다양한 혜택이 담긴<br>
			쿠폰 이벤트에 해당하는 제품을 주문해보세요.
		</p>
		<a href="/display/event?displayMenuId=event" class="btn_md_bordered">다양한 이벤트/행사 보러가기</a>
	</div>
{{/xif}}
{{#each this.regularEventRequesters}}
<dl>
	<dt>
		<b>{{ this.memberId }}</b>
		<small>{{ dateFormat this.requestDt }}</small>
		{{#xif this.recentRequestYn '==' 'Y'}}<span class="ico_new"><span class="sr_only">새글</span></span>{{/xif}}
		<!--<a href="#none" class="btn_delete"><i class="ico_del"></i><span class="sr_only">신청글 삭제</span></a>-->
		<span class="title">{{ this.requestTitle }}</span>
		<button type="button"><span class="sr_only">내용보기</span></button>
	</dt>
	<dd>
		<div class="panel gray">
			<p class="text">{{ this.requestReason }}</p>
		</div>
	</dd>
</dl>
{{/each}}
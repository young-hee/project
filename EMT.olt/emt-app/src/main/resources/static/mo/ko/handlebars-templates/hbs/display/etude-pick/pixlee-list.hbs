{{#each this.data}}
<li>
	<a href="#none">
		{{#xif this.content_type '==' 'video'}}
			<img src="{{ this.medium_url }}" alt="{{ decodeURIComponent this.alt_text }}" />
			<div class="play_button"></div>
		{{else}}
			<img src="{{ this.pixlee_cdn_photos.square_medium_url }}" alt="{{ decodeURIComponent this.alt_text }}" />
		{{/xif}}
	</a>
</li>
{{/each}}
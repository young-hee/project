{{#xif coupon.couponCnt '>' 0}}
	<div class="ui_accordion coupon_list availCoupon">
		<!--/* 쿠폰 한개 */-->
		{{#each coupon.list}}
			<div class="coupon_order">
				<div class="coupon_wrap">
					<div class="check_wrap">
						<input type="checkbox" id="{{ this.memberKeepingCouponSn }}" name="couponSn" data-code="{{ this.duplicationUseConditionCode }}">
						<label for="{{ this.memberKeepingCouponSn }}"></label>
					</div>
					<div class="info">
						<div class="flag_wrap">
							<span class="flag">{{ marketingOrg this.mktgOrgCode }}</span>
							<span class="flag">D-{{ this.expDDays }}</span>
						</div>
						<span class="coupon_name">{{ this.couponName }}</span>
						<div class="price_wrap">
							<span class="price">{{{ couponBenefitTypeCodeSwitch this }}}</span>	 <!-- //가격 숫자일때는 클래스 num -->
							<div class="ui_tooltip"><!-- //툴팁 -->
								<button type="button" class="btn_tooltip">툴팁보기</button>
								<span class="arr"></span>
								<div class="layer_tooltip">
									<dl>
										<dt>쿠폰 안내 <small>(만료예정일:{{ dateFormat this.expExpectedDt }})</small></dt>
										<dd>
											<ul class="list_bullet_dot">
												<li><pre>{{ this.foGuide }}</pre></li>
											</ul>
											<button type="button" class="ui_close sm"><span class="sr_only">툴팁 닫기</span></button>
										</dd>
									</dl>
								</div>
							</div><!-- //툴팁 -->
						</div><!-- //price_wrap -->
					</div><!-- //info -->
				</div><!-- //coupon_wrap -->
			</div>
		{{/each}}
		<!--/* 쿠폰 : 사은품 타입 */-->
	</div>
	<ul class="loading" id="couponApplyLoading" style="min-height: 100px; display: none">
		<li></li>
		<li></li>
		<li></li>
	</ul>
	<div class="page_btns">
		<button class="btn_lg_secondary" type="button" id="b_close">취소</button>
		<button class="btn_lg_neutral" type="button" id="b_save">적용</button>
	</div>
{{else}}
	<div class="panel notice">
		<i class="ico"></i>
		<p class="text font_lg"><b>보유중인 쿠폰이 없습니다.</b></p>
		<br>
		<p class="text">적용 가능 쿠폰보기에서 다운로드 가능한<br>
			쿠폰을 확인하시고 다양한 혜택이 담긴 쿠폰을<br>
			다운로드 받으실 수 있습니다.
		</p>
	</div>
{{/xif}}